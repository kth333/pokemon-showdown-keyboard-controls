{
    "manifest_version": 3,
    "name": "Pokémon Showdown Keyboard Controls",
    "version": "1.0",
    "description": "Adds keyboard controls to Pokémon Showdown.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://play.pokemonshowdown.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  